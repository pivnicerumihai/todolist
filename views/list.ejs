    <%- include ("header") -%>
    <div class="box" id="heading">
        <h1><%= listTitle %> To-Do List</h1>
    </div>
    <div class="box">

        <% todo.forEach(function(element,index){ %>
        <div class="item">
            <input type="checkbox">
            <p> <%= element %></p>
        </div>
        <%}) %>

    </div>

    <div cclass="item">
    <form  method="POST" action="/">
        <input type="text" name="todo" placeholder="New To Do Item" autocomplete="off">
        <button type="submit" name="list" value=<%= listTitle %>>+</button>
    </form>
</div>

